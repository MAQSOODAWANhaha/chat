# 智谱AI Realtime API配置
VITE_ZHIPU_API_KEY=your_api_key_here
VITE_ZHIPU_WS_URL=wss://open.bigmodel.cn/api/paas/v4/realtime

# 应用配置
VITE_APP_NAME=Realtime React
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=智谱AI Realtime API React前端项目

# 开发配置
VITE_DEV_PORT=3000
VITE_DEV_HOST=localhost

# 生产配置
VITE_API_BASE_URL=https://your-api-domain.com
VITE_WS_BASE_URL=wss://your-ws-domain.com

# 功能开关
VITE_ENABLE_SCREEN_SHARE=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_VOICE_RECORDING=true

# 音视频配置
VITE_DEFAULT_VIDEO_QUALITY=medium
VITE_DEFAULT_AUDIO_QUALITY=high
VITE_MAX_FILE_SIZE=10485760

# 日志配置
VITE_LOG_LEVEL=info